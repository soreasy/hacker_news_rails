<% if @password_error %>
  <p>
    Please enter correct password
  </p>
<% end %>

<% if @username_error %>
  <p>
    Please enter a valid username
  </p>
<% end %>

<%= form_for :user, url: loginn_path do |f| %>
  <p>
    <%= f.label :username %><br>
    <%= f.text_field :username %>
  </p>

  <p>
    <%= f.label :password %><br>
    <%= f.text_field :password %>
  </p>

  <p>
    <%= f.submit "Login" %>
  </p>
<% end %>
